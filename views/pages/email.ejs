<script>
    function AcceptReq(vl) {

        var res = vl.split("*");
        console.log(vl);
        var AppID=res[0];
        var name=res[1];
        var surname=res[2];
        var start=res[3];
        var sttime=res[4];
        var usemail=res[5];
        var t1=res[6];
        var t2=res[7];
        var t3=res[8];
        var end=res[9];
var drname=t1+" "+t2+" "+t3;


        $.ajax({
            url: '/email/update',
            type: 'post',
            data: {

                AppID: AppID


            },
            dataType: "json",
            async: true,

            // mongod is expecting the parameter name to be called "jsonp"

            error: function (XMLHttpRequest, textStatus, errorThrown) {
                console.log('error',errorThrown,textStatus);
            },

            success: function (data) {
                // title:data.title + data.start;
                //location.reload();


            },
        });
        $.get("/email/send_accept_meeting",{patientname:name+' '+surname,appdate:start,sttime:sttime,end:end,drname:drname,usemail:usemail},function(data) {
            if (data == "sent") {
                alert("Email sent");
//                //$("#message").html("Email is been sent at "+to+" . Please check inbox !");
//                $("#prev_b").hide();
//                $("#finish_b").hide();
//                $("#nav_bar_ap_form").hide();
//                $("#email_message").html("Email is been sent at "+to+" . Please check your inbox !");
            }
            else
                alert(data);
        });



    }

    function DeclineReq(id) {
        var AppID=id;
        $.ajax({
            url: '/email/decline',
            type: 'post',
            data: {

                AppID: AppID


            },
            dataType: "json",
            async: true,

            // mongod is expecting the parameter name to be called "jsonp"

            error: function (XMLHttpRequest, textStatus, errorThrown) {
                console.log('error',errorThrown,textStatus);
            },

            success: function (data) {
                // title:data.title + data.start;
                location.reload();


            },
        });
    }


</script>