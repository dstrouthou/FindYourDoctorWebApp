                        <div class="panel-title"><h4><%= user.fname%> <%= user.sname%> Appointment Requests</h4>
                        </div>
                        <div class="pull-right">
                            <div class="col-xs-12">
                                <input type="text" id="search-table" class="form-control pull-right" placeholder="Search">
                            </div>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="panel-body">


                        <table class="table table-hover demo-table-search" id="tableWithSearch" >
                            <thead>
                            <tr>
                                <th>Title</th>
                                <th>Name</th>
                                <th>Date</th>
                                <th>Start Time</th>
                                <th>End Time</th>
                                <th>Description</th>
                                <th>Actions</th>
                            </tr>
                            </thead>
                            <tbody>
                            <%for(var i=0;i<app.length;i++){%>

                            <tr>
                                <td class="v-align-middle semi-bold" >
                                    <p><%=app[i].title%></p>
                                </td>
                                <td class="v-align-middle" >
                                    <p><%=app[i].name%> <%=app[i].surname%></p>
                                </td>
                                <td class="v-align-middle" >
                                    <p><%=app[i].start%></p>
                                </td>
                                <td class="v-align-middle" >
                                    <p><%=app[i].sttime%></p>
                                </td>
                                <td class="v-align-middle" >
                                    <p><%=app[i].end%></p>
                                </td>
                                <td class="v-align-middle" >
                                    <p><%=app[i].description%></p>
                                </td>
                                <%if(app[i].drID==user.id){%>
                                <td class="v-align-middle" style="width: 150px">

                                    <button type="button" id="acceptreq" class="btn btn-success btn_accept_event"
                                    value= <%=app[i].AppID+"*"+app[i].name+"*"+app[i].surname+"*"+app[i].start+"*"+app[i].sttime+"*"+myinfo[0].email +"*"+"Dr"+"*"+user.fname+"*"+user.sname+"*"+app[i].end%> + onclick="AcceptReq(this.value)"  >
                                    <span class="glyphicon glyphicon-ok"></span>
                                    </button>
                                    <button type="button" id="acceptreq" class="btn btn-danger btn_accept_event" onclick="DeclineReq(this.value)"
                                            value=<%=app[i].AppID%>>
                                        <span class="glyphicon glyphicon-remove"></span>
                                    </button>

                                </td>
                                <%}%>
                                <%if(app[i].drID!=user.id){%>
                                <td class="v-align-middle" style="width: 150px">

                                   <p>Pending</p>

                                </td>
                                <%}%>
                            </tr>
                            <%}%>

                            </tbody>
                        </table>
                    </div>
                </div>
                <!-- END PANEL -->
            </div>


        </div>


