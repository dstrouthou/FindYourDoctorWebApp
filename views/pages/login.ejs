    <!-- show any messages that come back with authentication -->
    <% if (message.length > 0) { %>
    <div class="alert alert-danger"><%= message %></div>
    <% } %>
    <section id="main-content" style="height: 520px">


        <div class="register-container full-height sm-p-t-10">
            <div class="container-sm-height full-height">
                <div class="row row-sm-height">
                    <div class="col-sm-10 col-sm-height" style = "padding-top: 10px">
                        <!--<img src="assets/img/new.png" alt="logo" data-src="assets/img/new.png" data-src-retina="assets/img/new.png" width="430" height="80">-->
                        <h3 style="color:#0e90d2;"><b><center>FindYourDoctor is the solution for millions of health problems.</center></b></h3>

                        <form id="form-login" class="p-t-15" role="form" action="/login" method="post" >
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="form-group form-group-default">
                                        <label>Login</label>
                                        <input type="text" name="username" placeholder="Username" class="form-control" required>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="form-group form-group-default">
                                        <label>Password</label>
                                        <input type="password" name="password" placeholder="Password" class="form-control" required>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 no-padding">
                                    <div class="checkbox ">
                                        <input type="checkbox" value="1" id="checkbox1">
                                        <label for="checkbox1" style="color:#0e90d2;"><b>Keep Me Signed in</b></label>
                                    </div>
                                </div>
                                <!--<div class="col-md-6 text-right">-->
                                <!--<a href="#" class="text-info small">Help? Contact Support</a>-->
                                <!--</div>-->
                            </div>
                            <button class="btn btn-primary btn-cons m-t-10" type="submit" style="border-color: white; background-color: #0e90d2"><b>Sign in</b></button>
                        </form> <br>
                        <div class="pull-bottom sm-pull-bottom" style = "padding-bottom: 5px; color:#0e90d2;">
                            <p><b>Not already having an account?? </b><a href="/signup" style="color: #0e90d2; text-decoration: underline;"><b>Register</b></a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
